{
  "name": "payvault-react",
  "private": false,
  "url": "https://github.com/Project-Krypto/ReactPayVault",
  "homepage": "https://payvault-docs.vercel.app",
  "description": "Crypto Ecom Payment Processing",
  "version": "1.0.0",
  "main": "./dist/payvault-react.umd.js",
  "module": "./dist/payvault-react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/payvault-react.es.js",
      "require": "./dist/payvault-react.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": "19.0.0"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "watch": "vite build --watch",
    "build:storybook": "storybook build",
    "build:lib": "tsc && vite build",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^7.0.6",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^7.0.6",
    "@storybook/react-vite": "^7.0.6",
    "@storybook/testing-library": "^0.1.0",
    "@tanstack/react-query": "^4.26.1",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.27.3",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": ">=6",
    "jsdom": "^21.0.0",
    "json": "^11.0.0",
    "lint-staged": ">=10",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "storybook": "^7.0.6",
    "tailwindcss": "^3.2.4",
    "typescript": "*",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.27.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  }
}
